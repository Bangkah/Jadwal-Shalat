# Frontend Environment Variables
# Copy this file to .env and fill in your values

# Supabase Configuration
VITE_SUPABASE_URL=https://your-project-ref.supabase.co
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key

# Backend API URL
# Development
VITE_API_URL=http://localhost:8080
# Production (replace with your deployed backend URL)
# VITE_API_URL=https://your-backend-domain.com

# Optional: Development mode
VITE_DEV_MODE=true

# Optional: Enable debug logging
VITE_DEBUG=false

# Optional: App configuration
VITE_APP_NAME="Jadwal Shalat Indonesia"
VITE_APP_VERSION="2.0.0"

# Optional: Default location (Lhokseumawe)
VITE_DEFAULT_LATITUDE=5.1870
VITE_DEFAULT_LONGITUDE=97.1413
VITE_DEFAULT_CITY="Lhokseumawe"

# Optional: API timeout (in milliseconds)
VITE_API_TIMEOUT=10000

# Optional: Auto-refresh intervals (in milliseconds)
VITE_PRAYER_TIMES_REFRESH_INTERVAL=300000  # 5 minutes
VITE_CURRENT_PRAYER_REFRESH_INTERVAL=60000 # 1 minute

# Optional: Feature flags
VITE_ENABLE_GEOLOCATION=true
VITE_ENABLE_NOTIFICATIONS=false
VITE_ENABLE_DARK_MODE=false

# Optional: Analytics (if you use analytics)
# VITE_GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX
# VITE_MIXPANEL_TOKEN=your-mixpanel-token

# Optional: Error tracking (if you use Sentry)
# VITE_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id

# Optional: CDN URLs for assets
# VITE_CDN_URL=https://your-cdn-domain.com